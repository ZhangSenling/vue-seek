{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    // element特需配置
    ["es2015", {
        "modules": false
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-vue-jsx",
    "transform-runtime",
    [ "component", {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk"
      }
    ]],
  "env": {
    "test": {
      "presets": ["env", "stage-2"]
    }
  }
}
